type Details {
	detailsID: UUID!

	firstName: String!
	lastName: String!
	nickName: String
	gender: String!
	mobilePhoneNumber: String!
	createdAt: UnixTime!
}

type Student {
	studentID: String!

	details: Details!
	createdAt: UnixTime!

	bookings: [Booking!]!
	reviews: [Review!]!
}

type Instructor {
	instructorID: UUID!

	details: Details!
	photo: URL!
	createdAt: UnixTime!
}

type Location {
	locationID: UUID!

	name: String!
	plusCode: String!
	createdAt: UnixTime!
}

type Course {
	courseID: UUID!

	name: String!
	photo: URL!
	description: String!
	defaultPrice: PositiveInt
	defaultCapacity: PositiveInt!
	defaultDuration: PositiveInt!
	defaultLocation: Location!
	defaultEquipmentAvailable: PositiveInt
	createdAt: UnixTime!

	defaultInstructors: [Instructor!]!
	reviews: [Review!]!
}

type Session {
	sessionID: UUID!

	title: String!
	notes: String
	price: PositiveInt
	location: Location!
	startTime: UnixTime!
	endTime: UnixTime!
	capacity: PositiveInt!
	equipmentAvailable: NonNegativeInt!
	createdAt: UnixTime!

	course: Course!

	instructors: [Instructor!]!
	bookings: [Booking!]!
	capacityRemaining: NonNegativeInt!
}

type Booking {
	bookingID: UUID!

	notes: String!
	isBringingOwnEquipment: Boolean!
	createdAt: UnixTime!

	student: Student!
	session: Session!
}

type Review {
	reviewID: UUID!

	score: LikertScale!
	comment: String!
	createdAt: Int!

	course: Course!
	student: Student!
}
