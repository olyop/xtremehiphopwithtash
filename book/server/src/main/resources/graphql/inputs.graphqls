input GetSessionsInput {
	courseID: UUID
	startTime: UnixTime!
	endTime: UnixTime!
}

input DetailsInput {
	firstName: String!
	nickName: String
	lastName: String!
	gender: String
	mobilePhoneNumber: String!
	emailAddress: String!
	instagramUsername: String
}

input LocationInput {
	name: String!
	plusCode: String!
	address: String!
}

input InstructorInput {
	photo: URL!
	details: DetailsInput!
}

input CourseInput {
	name: String!
	photo: URL!
	description: String!
	defaultPrice: PositiveInt
	defaultEquipmentFee: PositiveInt
	defaultDuration: PositiveInt!
	defaultCapacityAvailable: PositiveInt!
	defaultEquipmentAvailable: PositiveInt
	defaultLocationID: UUID!
	defaultInstructorIDs: [UUID!]!
}

input SessionInput {
	title: String!
	notes: String
	startTime: UnixTime!
	endTime: UnixTime!
	courseID: UUID!
	price: PositiveInt
	equipmentFee: PositiveInt
	capacityAvailable: PositiveInt!
	equipmentAvailable: PositiveInt
	locationID: UUID!
	instructorIDs: [UUID!]!
}

enum PaymentMethod {
	CASH
	CARD
}

input BookingInput {
	notes: String
	sessionID: UUID!
	bookingQuantity: PositiveInt!
	equipmentQuantity: PositiveInt
	couponCode: String
	paymentMethod: PaymentMethod
}
