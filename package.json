	"volta": {
		"node": "19.4.0",
		"npm": "9.3.0"
	},
	"scripts": {
		"book-codegen": "graphql-codegen-esm -w",
		"book-clean": "rimraf book/server/src/main/resources/public",
		"book-client-tsc": "cd book/client && tsc --watch --noEmit",
		"book-client-dev": "cd book/client && npm run dev",
		"book-client-test": "cd book/client && npm run test",
		"book-client-build": "cd book/client && npm run build",
		"book-server-dev-build": "cd book/server && gradle classes --continuous -x test",
		"book-server-dev-run": "cd book/server && gradle bootRun --args='--spring.profiles.active=development'",
		"book-server-dev-stripe": "stripe listen --forward-to localhost:8080/stripe/webhook",
		"book-server-build-test": "cd book/server && gradle bootJar -Dspring.profiles.active=testing && cd ../..",
		"book-server-build": "cd book/server && gradle -Dspring.profiles.active=production bootJar && cd ../..",
		"book-dev": "clear && npm run book-clean && concurrently 'npm run book-codegen' 'npm run book-client-tsc' 'npm run book-client-dev' 'npm run book-server-dev-build' 'npm run book-server-dev-run' 'npm run book-server-dev-stripe'",
		"book-scp": "scp /home/op/code/xtremehiphopwithtash/book/server/build/libs/book.jar xtremehiphopwithtash:/home/ec2-user/book.jar",
		"book-build-public": "npm run book-clean && copy-and-watch 'book/client/dist/**' book/server/src/main/resources/public",
		"book-build-test": "npm run book-client-test && npm run book-build-public",
		"book-build": "npm run book-client-build && npm run book-build-public && npm run book-server-build",
		"book-test": "npm run book-build-test && npm run book-server-build-test && java -jar book/server/build/libs/book.jar",
		"book-deploy": "npm run book-scp",
		"book-build-deploy": "npm run book-build && npm run book-deploy",
		"git-push": "git add -A && git commit -m 'Update' && git push"
	},
	"devDependencies": {
		"@graphql-codegen/add": "5.0.0",
		"@graphql-codegen/cli": "4.0.1",
		"@graphql-codegen/client-preset": "4.0.0",
		"@graphql-codegen/near-operation-file-preset": "2.5.0",
		"@graphql-codegen/typescript": "4.0.0",
		"@graphql-codegen/typescript-resolvers": "4.0.0",
		"@graphql-eslint/eslint-plugin": "3.19.1",
		"@oly_op/cspell-dict": "^1.0.115",
		"@oly_op/eslint-config": "6.1.0",
		"@oly_op/eslint-config-graphql": "1.0.2",
		"@oly_op/eslint-config-react": "3.1.3",
		"@oly_op/prettier-config": "1.2.0",
		"@oly_op/stylelint-config": "2.2.0",
		"@oly_op/tsconfig": "1.0.25",
		"@types/eslint": "8.40.1",
		"@typescript-eslint/eslint-plugin": "5.59.9",
		"@typescript-eslint/parser": "5.59.9",
		"concurrently": "8.2.0",
		"copy-and-watch": "0.1.6",
		"cross-env": "7.0.3",
		"cspell": "^6.31.1",
		"del-cli": "5.0.0",
		"eslint": "8.42.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prefer-arrow-functions": "3.1.4",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-unicorn": "47.0.0",
		"graphql": "16.6.0",
		"npm-check-updates": "16.10.12",
		"prettier": "^2.8.8",
		"rimraf": "5.0.1",
		"rollup-plugin-visualizer": "5.9.2",
		"stylelint": "15.7.0",
		"stylelint-config-standard-scss": "9.0.0",
		"ts-node": "10.9.1",
		"typescript": "5.1.3"
	}
}
