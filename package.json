	"scripts": {
		"book-codegen": "graphql-codegen-esm -w",
		"book-check-ts": "cd book/client && tsc --watch --noEmit",
		"book-client": "cd book/client && npm run dev --  --force",
		"book-server-build": "cd book/server && gradle build --continuous -x test",
		"book-server-run": "cd book/server && gradle bootRun",
		"book": "concurrently 'npm run book-check-ts' 'npm run book-client' 'npm run book-server-build' 'npm run book-server-run' 'npm run book-codegen'"
	},
