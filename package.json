	},
	"scripts": {
		"book-client-dev-run": "cd book/client && npm run dev && cd ../..",
		"book-client-dev-codegen": "graphql-codegen-esm -w",
		"book-client-test-build": "cd book/client && npm run test && cd ../..",
		"book-client-test-deploy": "aws s3 sync --profile xtremehiphopwithtash --cache-control max-age=31536000 ./book/client/dist s3://xtremehiphopwithtash-dev.s3.ap-southeast-2.amazonaws.com",
		"book-client-prod-build": "cd book/client && npm run prod && cd ../..",
		"book-client-prod-deploy": "aws s3 sync --profile xtremehiphopwithtash --cache-control max-age=31536000 ./book/client/dist s3://xtremehiphopwithtash.s3.ap-southeast-2.amazonaws.com",
		"book-server-dev-watch": "cd book/server && gradle classes --continuous -x test -Dspring.profiles.active=development",
		"book-server-dev-run": "cd book/server && gradle bootRun --args='--spring.profiles.active=development'",
		"book-server-dev-stripe": "stripe listen -l -e payment_intent.succeeded --forward-to localhost:8080/stripe/webhook",
		"book-server-test-build": "cd book/server && gradle bootJar -Dspring.profiles.active=testing && cd ../..",
		"book-server-test-deploy": "scp ./book/server/build/libs/book.jar xtremehiphopwithtash-dev:/home/ec2-user/book.jar",
		"book-server-prod-build": "cd book/server && gradle bootJar -Dspring.profiles.active=production && cd ../..",
		"book-server-prod-deploy": "scp ./book/server/build/libs/book.jar xtremehiphopwithtash:/home/ec2-user/book.jar",
		"book-dev": "concurrently 'npm run book-client-dev-codegen' 'npm run book-client-dev-run' 'npm run book-server-dev-watch' 'npm run book-server-dev-run' 'npm run book-server-dev-stripe'",
		"book-test": "npm run book-client-test-build && npm run book-server-test-build && npm run book-client-test-deploy && npm run book-server-test-deploy",
		"book-prod": "npm run book-client-prod-build && npm run book-server-prod-build && npm run book-client-prod-deploy && npm run book-server-prod-deploy"
	},
	"devDependencies": {
		"@graphql-codegen/add": "5.0.0",
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/client-preset": "4.1.0",
		"@graphql-codegen/near-operation-file-preset": "3.0.0",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-resolvers": "4.0.1",
		"@oly_op/cspell-dict": "^1.1.2",
		"@oly_op/eslint-config": "6.5.3",
		"@oly_op/eslint-config-graphql": "1.1.0",
		"@oly_op/eslint-config-react": "4.0.0",
		"@oly_op/prettier-config": "1.7.8",
		"@oly_op/tsconfig": "1.2.0",
		"@parcel/watcher": "2.3.0",
		"concurrently": "8.2.2",
		"copy-and-watch": "0.1.6",
		"cross-env": "7.0.3",
		"graphql": "16.8.1",
		"rimraf": "5.0.5",
		"rollup-plugin-visualizer": "5.10.0",
		"ts-node": "10.9.1"
	}
}
